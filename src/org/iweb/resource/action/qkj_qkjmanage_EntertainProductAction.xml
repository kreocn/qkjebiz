<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="qkjmanage_entertProduct" extends="default" namespace="/qkjmanage">
		<default-interceptor-ref name="myStackIndex" />
		<!--  Start -->
		<action name="entertProduct_add" class="com.qkj.manage.action.EntertProductAction" method="add">
			<result type="redirectAction">
				<param name="actionName">entert_load</param>
				<param name="entert.uuid">${entertProduct.enter_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		<action name="entertProduct_del" class="com.qkj.manage.action.EntertProductAction" method="del">
			<result type="redirectAction">
				<param name="actionName">entert_load</param>
				<param name="entert.uuid">${entertProduct.enter_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		
		<action name="entertMem_add" class="com.qkj.manage.action.EntertProductAction" method="addMem">
			<result type="redirectAction">
				<param name="actionName">entert_load</param>
				<param name="entert.uuid">${entertMem.enter_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		<action name="entertMem_del" class="com.qkj.manage.action.EntertProductAction" method="delMem">
			<result type="redirectAction">
				<param name="actionName">entert_load</param>
				<param name="entert.uuid">${entertMem.enter_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		<!--  End -->
	</package>
</struts>
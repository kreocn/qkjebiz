<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="qkjmanage_ClosePosm" extends="default" namespace="/qkjmanage">
		<default-interceptor-ref name="myStackIndex" />
		<!-- ActivePosm Start -->
		<action name="closePosm_add" class="com.qkj.manage.action.CloseOrderPosmAction" method="add">
			<result type="redirectAction">
				<param name="actionName">closeOrder_load</param>
				<param name="closeOrder.uuid">${closePosm.closeOrder_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		<action name="closePosm_del" class="com.qkj.manage.action.CloseOrderPosmAction" method="del">
			<result type="redirectAction">
				<param name="actionName">closeOrder_load</param>
				<param name="closeOrder.uuid">${closePosm.closeOrder_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>

		<!-- ActivePosm End -->
	</package>
</struts>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="qkjmanage_applyProduct" extends="default" namespace="/qkjmanage">
		<default-interceptor-ref name="myStackIndex" />
		<!-- Apply Start -->
		<action name="applyProduct_add" class="com.qkj.manage.action.ApplyProductAction" method="add">
			<result type="redirectAction">
				<param name="actionName">apply_load</param>
				<param name="apply.uuid">${applyProduct.apply_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		
		
				<action name="applyProduct_del" class="com.qkj.manage.action.ApplyProductAction" method="del">
					<result type="redirectAction">
				<param name="actionName">apply_load</param>
				<param name="apply.uuid">${applyProduct.apply_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		
	    <!-- Apply end -->
	</package>
</struts>
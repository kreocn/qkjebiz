<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="qkjmanage_closeOrderMemcost" extends="default" namespace="/qkjmanage">
		<default-interceptor-ref name="myStackIndex" />
		<!-- CloseOrderMemcost Start -->
		<action name="closeOrderMemcost_add" class="com.qkj.manage.action.CloseOrderMemcostAction" method="add">
			<result type="redirectAction">
				<param name="actionName">closeOrder_load</param>
				<param name="closeOrder.uuid">${closeOrderMemcost.close_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		<action name="closeOrderMemcost_del" class="com.qkj.manage.action.CloseOrderMemcostAction" method="del">
			<result type="redirectAction">
				<param name="actionName">closeOrder_load</param>
				<param name="closeOrder.uuid">${closeOrderMemcost.close_id}</param>
				<param name="viewFlag">mdy</param>
				<param name="message">${message}</param>
			</result>
		</action>
		
		<!-- closeOrderMemcost End -->
	</package>
</struts>
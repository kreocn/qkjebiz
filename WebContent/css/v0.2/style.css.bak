}

.op-area a {
	display: inline-block; vertical-align: middle; white-space: nowrap; word-break: break-all;
}

input[type="button"].input-nostyle,input[type="submit"].input-nostyle,input[type="reset"].input-black,a.input-nostyle {
	background-color: #FFF; border-color: #363636; color: #363636;
}

input[type="button"].input-gray,input[type="submit"].input-gray,input[type="reset"].input-gray,a.input-gray {
	background-color: #999; border-color: #999;
}

input[type="button"].input-red,input[type="submit"].input-red,input[type="reset"].input-red,a.input-red {
	background-color: #cc0055; border-color: #cc0055;
}

input[type="button"].input-green,input[type="submit"].input-green,input[type="reset"].input-green,a.input-green {

	/*min-height:500px;
	height:auto;*/
}

.ht_sjsub li {
	float: left; width: 120px; font-size: 14px; line-height: 30px;
}

.ht_sjsub a {
	display: block; padding: 0 15px; color: #444; font-size: 15px;
}

.ht_sjsub a.sjhv {
	background: #ecfbd4; color: #059c77;
}

.ht_clicka {
	height: 45px; display: block;
}

footer {
	position: fixed; bottom: 0; z-index: 9999; width: 100%; height: 25px; line-height: 25px; background: #363636; color: #fff;
}

.botmsg {
	float: right; padding-right: 0.8em; font-size: 14px;
}

.tab_right {
	float: right; width: 84%; margin-right: 0.8em; font-size: 14px;
}

.layout_iframe {
	width: 100%; height: 100%; overflow: auto; border: none;
}

@media screen and (max-width:1180px) {
	.yesnull,.nonull,.success,.nosuc,.daisuc {
		margin-top: 6px;
	}
	.td5 {
		display: none;
	}
	.tab_warp .td0 {
		display: table-cell;
	}
	.lb_lgsfy {
		width: 49.89%;
	}
	.main {
		width: 100%;
	}
	.label_hang_linput {
		width: 800px !important;
	}
}

@media screen and (max-width:1050px) {
	.lb_lgsfy {
		width: 49.79%;
	}
}

@media screen and (max-width:960px) {
	.ht_rczuo {
		display: none;
	}
	.ht_rset {
		display: block;
	}
	.ui-panel {
		width: 11.8em !important;
	}
	.tab_right {
		width: 100%; margin: 0;
	}
	.ht_lTit,.td4,.fy_hide {
		display: none;
	}
	.lb_gsfy {
		width: 100%;
	}
	.lb_lgsfy {
		width: 100%; border: 0; border-bottom: 1px solid #ccc;
	}
	.tab_warp {
		width: 100%;
	}
	.dq_step {
		padding: 10px;
	}
	.label_con {
		width: 100%; float: left;
	}
	.ht_lMenu {
		display: block; width: 36px; height: 36px; position: absolute; left: 15px; top: 3px; margin-top: 0px; padding-left: 0px; text-indent: -9999px;
		background-image: url(../../images/white/crate36x36.png); cursor: pointer;
	}
	.label_hang_linput {
		width: 550px !important;
	}
}

@media screen and (max-width:768px) {
	.ht_lTit,.td3 {
		display: none;
	}
	.label_hang {
		clear: both;
	}
}

@media screen and (max-width:655px) {
	.ht_lTit,.lb_text2 {
		width: 300px;
	}
	.zhuangtai {
		clear: both; margin-bottom: 5px;
	}
	.label_hang_linput {
		width: 300px !important;
	}
}

@media screen and (max-width:550px) {
	.ht_lTit,.td2 {
		display: none;
	}
	.label_hang {
		clear: both;
	}
	.label_con { /*width: 440px;*/
		
	}
	.lb_text2 {
		width: 300px;
	}
	.zhuangtai {
		clear: both; margin-bottom: 5px;
	}
}

@media screen and (max-width:420px) {
	.label_rwben2 {
		width: 107px;
	}
	.label_hang_linput {
		width: 170px !important;
	}
}

@media print {
	.printarea {
		display: block;
	}
	.noprint {
		display: none;
	}
	.ui-layout-south {
		display: none;
	}
	.ui-layout-north {
		display: none;
	}
	.ui-layout-east {
		display: none;
	}
	.printshow {
		display: table-row-group !important;
	}
	input,textarea,select {
		border: none !important; background: transparent;
	}
	.pt {
		font-size: 16px; font-weight: bold;
	}
	.dq_step {
		border-bottom: #666 solid 1px;
	}
	.main {
		padding-top: 55px; background: url("../../images/print/pageheader02.png") transparent no-repeat 0 0;
	}
}
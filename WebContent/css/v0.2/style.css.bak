	background-color: #3477a1; border-color: #3477a1;
}

.input-a input:hover,.op-area a:hover,.input-black input:hover,.input-gray input:hover,.input-red input:hover,.input-green a:hover,.input-yellow a:hover,.input-blue a:hover,.regular-checkbox:hover,.regular-radio:hover,.iselect select:hover
	{
	opacity: 0.75;
}
/*checkbox*/
.regular-checkbox {
	display: none;
}

.regular-checkbox+label {
	background-color: #fafafa; border: 1px solid #cacece; box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05);
	padding: 8px; border-radius: 3px; display: inline-block; position: relative; top: 5px; margin-right: 5px; left: 2px; line-height: 16px;
}

.regular-checkbox+label:active,.regular-checkbox:checked+label:active {
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px 1px 3px rgba(0, 0, 0, 0.1);
}

.regular-checkbox:checked+label {
	background-color: #e9ecee; border: 1px solid #adb8c0; box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.1);
	color: #99a1a7;
}

.regular-checkbox:checked+label:after {
	content: '\2714'; font-size: 14px; position: absolute; top: 0px; left: 2px; color: #99a1a7;
}
/*radio*/
.regular-radio {
	display: none;
}

.regular-radio+label {
	-webkit-appearance: none; background-color: #fafafa; border: 1px solid #cacece;
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05); padding: 8px; border-radius: 50px; display: inline-block;
	position: relative; top: 5px; margin-right: 5px; left: 2px;
}

.regular-radio:checked+label:after {
	content: ' '; width: 10px; height: 10px; border-radius: 50px; position: absolute; top: 3px; background: #99a1a7;
	box-shadow: inset 0px 0px 10px rgba(0, 0, 0, 0.3); text-shadow: 0px; left: 3px; font-size: 32px;
}

.regular-radio:checked+label {
	background-color: #e9ecee; color: #99a1a7; border: 1px solid #adb8c0;
	box-shadow: box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.1);;
}

.regular-radio+label:active,.regular-radio:checked+label:active {
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px 1px 3px rgba(0, 0, 0, 0.1);
}
/*select*/
.iselect {
	display: inline-block; overflow: hidden; position: relative; background-color: white; border: 1px solid #fefefe;
	box-shadow: 0px 0px 5px 0px #909090 inset; border-radius: 0.4em; padding: 0 3px 0 0; -o-text-overflow: ellipsis; text-overflow: ellipsis;
	height: 28px; line-height: 28px; overflow: hidden;
}

.iselect select {
	width: 100%; border: none; background: transparent; background-image: none; outline: none; height: 24px; line-height: 24px; position: relative;
	top: 2px; padding: 0px 3px 0 8px;
}

.iselect:before,.iselect:after {
	content: ''; display: block; position: absolute; right: 5px; top: 2px; pointer-events: none;
}

.iselect:after {
	position: absolute; right: 5px; width: 18px; height: 24px; background-color: #FFF; border-left: 1px solid #eee;
}

.iselect:before {
	top: 50%; border: solid transparent; border-width: 0.25em 0.25em 0; border-top-color: #ccc; margin: -0.125em 0.3em 0 0.1em; z-index: 999;
}
